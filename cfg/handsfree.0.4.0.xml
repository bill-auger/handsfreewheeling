	<!-- 2do repack
	<binding input="loop-clicked"
			conditions="button=3 and down=1"
			output1="erase-loop" parameters1="loopid=loopid"/>
-->
<!-- 2do redefine keys ??? same def proc for manual select and manual pause
    <binding input="key"
			conditions="key=comma and keydown=1 and
						VAR_keyheld_shift=1"
			output1="erase-loop" parameters1="loopid=38"/>
	<binding input="key"
			conditions="key=96>122 and keydown=1 and
						VAR_keyheld_shift=1"
			output1="erase-loop" parameters1="loopid=key"/>
-->
	<!-- HELP: <ESC>: Undo Youngest Loop -->
<!-- perhaps also do something dryer (FLAG_set_fills mod 10^VAR_next_loopset) -->
	<binding input="key" conditions="key=escape and keydown=1 and SYSTEM_num_recording_loops_in_map=1>8"
		output1="erase-loop" parameters1="loopid=VAR_this_loopid"
		output2="set-variable" parameters2="var=VAR_this_loopid and value=VAR_this_loopid - 1"/>
	<binding input="key" conditions="key=escape and keydown=1 and
			TOGGLE_loopset=VAR_prev_loopset and VAR_prev_fill=2>8"
		output1="set-variable" parameters1="var=VAR_prev_fill and value=VAR_prev_fill - 1"
		output2="go-sub" parameters2="sub=10"/>
	<binding input="key" conditions="key=escape and keydown=1 and
			TOGGLE_loopset=VAR_prev_loopset and VAR_prev_fill=0>1"
		output1="set-variable" parameters1="var=VAR_prev_fill and value=0"
		output2="go-sub" parameters2="sub=10"/>
	<binding input="key" conditions="key=escape and keydown=1"
		output1="go-sub" parameters1="sub=10"/>

	<binding input="go-sub" conditions="sub=10 and TOGGLE_loopset=1 and TOGGLE_set1_fill=2>8"
		output1="erase-loop" parameters1="loopid=10 + TOGGLE_set1_fill - 1"
		output2="set-variable" parameters2="var=TOGGLE_set1_fill and value=TOGGLE_set1_fill - 1"/>
	<binding input="go-sub" conditions="sub=10 and TOGGLE_loopset=2 and TOGGLE_set2_fill=2>8"
		output1="erase-loop" parameters1="loopid=20 + TOGGLE_set2_fill - 1"
		output2="set-variable" parameters2="var=TOGGLE_set2_fill and value=TOGGLE_set2_fill - 1"/>
	<binding input="go-sub" conditions="sub=10 and TOGGLE_loopset=3 and TOGGLE_set3_fill=2>8"
		output1="erase-loop" parameters1="loopid=30 + TOGGLE_set3_fill - 1"
		output2="set-variable" parameters2="var=TOGGLE_set3_fill and value=TOGGLE_set3_fill - 1"/>
	<binding input="go-sub" conditions="sub=10 and TOGGLE_loopset=1 and TOGGLE_set1_fill=0>1"
		output1="delete-pulse" parameters1="pulse=0"
		output2="set-variable" parameters2="var=TOGGLE_set1_fill and value=0"
		output3="go-sub" parameters3="sub=11"/>
	<binding input="go-sub" conditions="sub=10 and TOGGLE_loopset=2 and TOGGLE_set2_fill=0>1"
		output1="delete-pulse" parameters1="pulse=1"
		output2="set-variable" parameters2="var=TOGGLE_set2_fill and value=0"
		output3="go-sub" parameters3="sub=11"/>
	<binding input="go-sub" conditions="sub=10 and TOGGLE_loopset=3 and TOGGLE_set3_fill=0>1"
		output1="delete-pulse" parameters1="pulse=2"
		output2="set-variable" parameters2="var=TOGGLE_set3_fill and value=0"
		output3="go-sub" parameters3="sub=11"/>

<!-- this may also work if pulses= 1, 10, 100
			output4="delete-pulse"
			parameters4="pulse=TOGGLE_set1_fill * 10 ^ VAR_next_loopset"/>


output2="set-variable" parameters2="var=VAR_this_loopid and value=VAR_this_loopid - 1"/>
